package(default_visibility = ["//visibility:private"])

licenses(["notice"])

cc_library(
    name = "freetype",
    deps = [
        ":libbz2",
        ":libfreetype",
        ":libpng",
    ],
    visibility = ["//visibility:public"],
)

cc_import(
    name = "libbz2",
    static_library = "libbz2.a",
)

cc_import(
    name = "libfreetype",
    static_library = "libfreetype.a",
)

cc_import(
    name = "libpng",
    static_library = "libpng.a",
)